function draw_slide(index)
 cls()
 slide = slides[index]
 for line in all(slide) do
	if line[3] then
		print(line[1], 0, line[3], line[2])
	else
		print(line[1], line[2])
	end

 end
end

function draw_number()
str = current.."/"..#slides
print(str, 128-4*#str, 128-8, 1)
end

function _draw()
	cls()
	draw_slide(current)
	draw_number()
end

function increase()
	current = max(1, (current + 1) % (#slides + 1))
		sfx(current)
end

function decrease()
	current = current - 1
	if current < 1 then
		current = #slides
	end
		sfx(current)
end

function _update()
	if btnp(1) then
		increase()
	end
	if btnp(0) then
		decrease()
	end
end


